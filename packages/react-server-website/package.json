{
  "name": "react-server-website",
  "version": "0.0.1",
  "description": "A react-server instance",
  "main": "HelloWorld.js",
  "private": true,
  "scripts": {
    "build-assets": "npm run docs && node ./cli.js --compile-only --minify",
    "start-prod": "node ./cli.js --port 3010 --minify --js-url /assets/",
    "start": "node ./cli.js --port 3010 --js-port 3011",
    "test": "eslint routes.js gulpfile.js pages/ components/ && nsp check",
    "docs": "docco -o docs ./components/*.js ./lib/*.js ./middleware/*.js ./pages/*.js *.js && node build-dir-contents.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "highlight.js": "^9.5.0",
    "react": "~0.14.2",
    "react-dom": "~0.14.2",
    "react-server": "^0.3.4",
    "react-server-cli": "^0.4.0",
    "react-server-data-bundle-cache": "^0.3.4",
    "remarkable": "^1.6.2",
    "superagent": "1.2.0"
  },
  "devDependencies": {
    "babel-preset-react-server": "^0.3.3",
    "docco": "^0.7.0",
    "nsp": "^2.3.3"
  }
}
