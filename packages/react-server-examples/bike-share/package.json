{
  "name": "react-server-bike-share",
  "version": "0.0.1",
  "private": true,
  "description": "A react-server instance",
  "main": "HelloWorld.js",
  "author": "Doug Wade <doug.wade@redfin.com>",
  "scripts": {
    "clean": "rm -rf build __clientTemp",
    "start": "npm run clean && npm run styles && react-server start",
